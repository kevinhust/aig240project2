#!/usr/bin/expect -f
set password "123"
set host "192.168.64.4"
set user "jetauto"
set timeout 600

set cmd {
rm -rf ~/jetauto_ws/src/jetauto_simulations && \
cd ~/jetauto_ws/src && \
git clone https://github.com/hiwonder/jetauto_simulation.git
}

spawn ssh -t -o StrictHostKeyChecking=no $user@$host $cmd
expect {
    "password:" {
        send "$password\r"
        exp_continue
    }
    eof
}
